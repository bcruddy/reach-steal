{
    "globals": {},
    "testRegex": "\\.(test|spec)\\.js$",
    "moduleFileExtensions": ["js"],
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
        "src/**/*.js"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 0,
            "functions": 0,
            "lines": 0,
            "statements": 0
        }
    }
}
